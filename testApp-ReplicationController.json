{
   "kind":"ReplicationController",
   "apiVersion":"v1",
   "metadata":{
      "name":"msventas",
      "labels":{
         "app":"msventas"
      }
   },
   "spec":{
      "replicas":3,
      "selector":{
         "app":"msventas"
      },
      "template":{
         "metadata":{
            "labels":{
               "app":"msventas"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"msventas",
                  "image":"184013750779.dkr.ecr.us-east-2.amazonaws.com/msventas:latest",
                  "ports":[
                     {
                        "name":"http-server",
                        "containerPort":443
                     }
                  ]
               }
            ]
         }
      }
   }
}
